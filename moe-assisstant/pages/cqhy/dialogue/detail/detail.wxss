.container {
  padding: 0;
}

.container .content {
  padding: 20rpx 40rpx;
  height: calc(100vh - 100px);
  overflow: scroll;
}

.container .content .header {
  height: 300rpx;
  border: 1px solid #303133;
  border-radius: 10rpx;
  display: flex;
}

.container .content .header .header-left {
  display: flex;
  flex-direction: column;
  width: 40%;
  box-sizing: border-box;
  border-right: 1px solid #303133;
  height: inherit;
  overflow: hidden;
}

.container .content .header .header-left .thumbnail {
  width: 100%;
  height: 100%;
}

.container .content .header .header-right {
  width: 70%;
  height: inherit;
  display: flex;
  flex-direction: column;
}

.container .content .header .header-right .header-item {
  box-sizing: border-box;
  border-bottom: 1px solid #303133;
  height: initial;
  flex-grow: 1;
  width: 100%;
  display: flex;
  align-items: center;
  padding-left: 20rpx;
  font-size: 26rpx;
  height: 100%;
}

.container .content .header .header-right .header-item:last-of-type {
  border-bottom: unset;
}

.container .content .header .header-right .header-item .header-item-title {
  min-width: 30%;
  box-sizing: border-box;
  height: 100%;
  display: flex;
  align-items: center;
  max-width: 30%;
  border-right: 1px solid #303133;
}

.container .content .header .header-right .header-item .header-item-content {
  display: flex;
  align-items: center;
  justify-content: center;
  flex-grow: 1;
  font-size: 24rpx;
  margin: 20rpx 0;
  padding: 0 20rpx;
  overflow: scroll;
  height: 100%;
}

.container .content .header .header-right .header-item .unalign {
  align-items: baseline;
}

.container .content .header .header-right .header-item .language-switch-container switch {
  margin-left: 10rpx;
  transform: scale(0.7);
}

.container .content .body {
  margin-top: 40rpx;
}

.container .content .body .reaction-scripts {
  border: 1px solid #909999;
  border-radius: 10rpx;
  padding: 20rpx 0;
}

.container .content .body .reaction-scripts .reaction-script-group-container {
  display: flex;
  flex-direction: column;
  justify-content: center;
  align-items: center;
  margin-bottom: 160rpx;
}

.container .content .body .reaction-scripts .reaction-script-group-container:last-of-type {
  margin-bottom: unset;
}

.container .content .body .reaction-scripts .reaction-script-group-container .reaction-script-group-title {
  font-size: 60rpx;
  margin-bottom: 40rpx;
}

.container .content .body .reaction-scripts .reaction-script-group-container .reaction-container {
  display: flex;
  flex-direction: column;
  width: 100%;
}

.container .content .body .reaction-scripts .reaction-script-group-container .reaction-container .reaction-item {
  width: 100%;
}

.container .content .body .reaction-scripts .reaction-script-group-container .reaction-container .reaction-item .reaction {
  display: flex;
  align-items: center;
  position: relative;
  box-sizing: border-box;
  height: 144px;
  margin-bottom: 20rpx;
}

.container .content .body .reaction-scripts .reaction-script-group-container .reaction-container .reaction-item .reaction:last-of-type {
  margin-bottom: unset;
}

.container .content .body .reaction-scripts .reaction-script-group-container .reaction-container .reaction-item .reaction .thumbnail {
  min-width: 240px;
  min-height: 240px;
  border-radius: 100%;
}

.container .content .body .reaction-scripts .reaction-script-group-container .reaction-container .reaction-item .reaction .left-thumbnail {
  transform: scale(0.6) translate(-80px, 0);
  margin-left: 10px;
}

.container .content .body .reaction-scripts .reaction-script-group-container .reaction-container .reaction-item .reaction .right-thumbnail {
  transform: scale(0.6) translate(-28px, 0);
}

.container .content .body .reaction-scripts .reaction-script-group-container .reaction-container .reaction-item .reaction .right-text {
  box-shadow: 1px 1px 1px #909999;
  transform: translate(-76px, 0);
}

.container .content .body .reaction-scripts .reaction-script-group-container .reaction-container .reaction-item .reaction .right-text::before {
  content: '';
  position: absolute;
  left: -11px;
  top: 30%;
  width: 0;
  height: 0;
  border-top: 10px solid transparent;
  border-right: 10px solid #909999;
  border-bottom: 10px solid transparent;
}

.container .content .body .reaction-scripts .reaction-script-group-container .reaction-container .reaction-item .reaction .right-text::after {
  content: '';
  position: absolute;
  left: -9px;
  top: 30%;
  width: 0;
  height: 0;
  border-top: 10px solid transparent;
  border-right: 10px solid #f2f6fc;
  border-bottom: 10px solid transparent;
}

.container .content .body .reaction-scripts .reaction-script-group-container .reaction-container .reaction-item .reaction .left-text::before {
  content: '';
  position: absolute;
  right: -11px;
  top: 30%;
  width: 0;
  height: 0;
  border-top: 10px solid transparent;
  border-left: 10px solid #909999;
  border-bottom: 10px solid transparent;
}

.container .content .body .reaction-scripts .reaction-script-group-container .reaction-container .reaction-item .reaction .left-text::after {
  content: '';
  position: absolute;
  right: -9px;
  top: 30%;
  width: 0;
  height: 0;
  border-top: 10px solid transparent;
  border-left: 10px solid #f2f6fc;
  border-bottom: 10px solid transparent;
}

.container .content .body .reaction-scripts .reaction-script-group-container .reaction-container .reaction-item .reaction .left-text {
  box-shadow: -1px 1px 1px #909999;
  transform: translate(10px, 0);
}

.container .content .body .reaction-scripts .reaction-script-group-container .reaction-container .reaction-item .reaction .script-content {
  border: 1px solid #909999;
  border-radius: 10rpx;
  padding: 20rpx 40rpx;
  font-size: 32rpx;
  min-width: calc(100% - 144px - 10px - 48px - 10px - 48px + 30px);
}

.container .illustration-container {
  position: fixed;
  top: 0;
  bottom: 0;
  left: 0;
  right: 0;
  background-color: rgba(0, 0, 0, 0.5);
  z-index: 100;
  display: flex;
  justify-content: center;
  align-items: center;
}

.container .illustration-container .illustration-panel {
  height: 80vh;
  width: 80vw;
  background-color: white;
  border-radius: 10rpx;
  position: relative;
}

.container .illustration-container .illustration-panel .illustration {
  height: inherit;
  width: inherit;
}

.container .illustration-container .illustration-panel .button {
  position: absolute;
  top: calc(50% - 40rpx);
  width: 80rpx;
  height: 80rpx;
  background-size: 80rpx 80rpx;
  background-repeat: no-repeat;
  background-position: 0 0;
}

.container .illustration-container .illustration-panel .go-pre {
  left: 10rpx;
  background-image: url('data:image/svg+xml;base64,');
}

.container .illustration-container .illustration-panel .go-next {
  right: 10rpx;
  background-image: url('data:image/svg+xml;base64,');
}

.container .gesture-container .gesture-icon {
  width: 150px;
  height: 150px;
  position: fixed;
  transform: rotate(180deg);
}

.container .gesture-container .start-point {
  width: 30px;
  height: 30px;
  border-radius: 100%;
  background-color: #409EFF;
  position: fixed;
}

.container .gesture-container .touch-line {
  position: fixed;
  background-color: #409EFF;
  height: 10px;
}

.container .gesture-container .arrow {
  position: fixed;
  font-size: 80px;
  color: #409EFF;
}