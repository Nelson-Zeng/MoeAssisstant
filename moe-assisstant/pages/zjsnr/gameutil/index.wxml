<view class="container">
  <view wx:for="{{utilList}}" class="util-group" wx:key="index">
    <view wx:for="{{item}}" wx:for-item="util" wx:key="index" class="util-item" bindtap="callUtil" data-content="{{util}}">
      <view class="util-icon" style="background-image: {{util.icon}}"></view>
      <label class="util-title">{{util.title}}</label>
    </view>
  </view>
</view>