.container {
  padding: unset;
  width: 100%;
  overflow: scroll;
}

.container .content-body {
  padding: 20rpx;
}

.container .content-body .hint {
  padding: 10rpx;
  display: flex;
  justify-content: flex-end;
}

.container .content-body .map-content-item {
  margin-bottom: 50rpx;
}

.container .content-body .map-content-item .map-title {
  font-size: 40rpx;
}

.container .content-body .map-content-item image {
  width: 100%;
  height: 400rpx;
  margin: 20rpx 0;
}

.container .content-body .map-content-item .detail-area .detail-table {
  border: 1rpx solid #606266;
  box-sizing: border-box;
}

.container .content-body .map-content-item .detail-area .detail-table .first-type {
  display: flex;
  align-items: center;
  min-height: 100%;
  border-bottom: 1px solid #606266;
  box-sizing: border-box;
}

.container .content-body .map-content-item .detail-area .detail-table .first-type .first-type-title {
  width: 30%;
  text-align: center;
}

.container .content-body .map-content-item .detail-area .detail-table .first-type .first-type-content {
  border-left: 1rpx solid #606266;
  width: 70%;
  padding: 6rpx 10rpx;
  box-sizing: border-box;
}

.container .content-body .map-content-item .detail-area .detail-table .second-type {
  display: flex;
  align-items: center;
  border-bottom: 1px solid #606266;
  box-sizing: border-box;
}

.container .content-body .map-content-item .detail-area .detail-table .second-type-header {
  display: flex;
}

.container .content-body .map-content-item .detail-area .detail-table .second-type-header .second-type-header-item {
  width: 10%;
  box-sizing: border-box;
  border-left: 1px solid #606266;
  border-bottom: 1px solid #606266;
  display: flex;
  justify-content: center;
}

.container .content-body .map-content-item .detail-area .detail-table .second-type-header .second-type-header-item:first-of-type {
  border-left: none;
}

.container .content-body .map-content-item .detail-area .detail-table .second-type-header .second-type-display {
  width: 30%;
}

.container .content-body .map-content-item .detail-area .detail-table .second-type .second-type-left {
  width: 10%;
  text-align: center;
}

.container .content-body .map-content-item .detail-area .detail-table .second-type .second-type-middle {
  width: calc(80% + 1px);
  border-left: 1px solid #606266;
  border-right: 1px solid #606266;
  box-sizing: border-box;
}

.container .content-body .map-content-item .detail-area .detail-table .second-type .second-type-middle .second-type-item {
  display: flex;
  align-items: center;
  border-bottom: 1px solid #606266;
}

.container .content-body .map-content-item .detail-area .detail-table .second-type .second-type-middle .second-type-item:last-child {
  border-bottom: unset;
}

.container .content-body .map-content-item .detail-area .detail-table .second-type .second-type-middle .second-type-item .second-type-title {
  width: calc(12.5% - 1px);
  box-sizing: border-box;
  display: flex;
  justify-content: center;
  align-items: center;
}

.container .content-body .map-content-item .detail-area .detail-table .second-type .second-type-middle .second-type-item .second-type-param {
  width: 10%;
  text-align: center;
  border-left: 1px solid #606266;
}

.container .content-body .map-content-item .detail-area .detail-table .second-type .second-type-middle .second-type-item .second-type-param:first-of-type {
  border-left: none;
}

.container .content-body .map-content-item .detail-area .detail-table .second-type .second-type-middle .second-type-item .second-type-setting {
  width: calc(90% - 1px);
  border-left: 1px solid #606266;
  box-sizing: border-box;
  display: flex;
}

.container .content-body .map-content-item .detail-area .detail-table .second-type .second-type-middle .second-type-item .second-type-setting .second-type-setting-param {
  width: calc(100% / 7);
  display: flex;
  justify-content: center;
  align-items: center;
}

.container .content-body .map-content-item .detail-area .detail-table .second-type .second-type-middle .second-type-item .second-type-setting .second-type-setting-tracking {
  border-left: 1px solid #606266;
}

.container .content-body .map-content-item .detail-area .detail-table .second-type .second-type-middle .second-type-item .second-type-setting .second-type-setting-antiAircraft {
  border-left: 1px solid #606266;
}

.container .content-body .map-content-item .detail-area .detail-table .second-type .second-type-middle .second-type-item .second-type-setting .second-type-setting-display {
  min-width: calc(100% / 7 * 3 - 1px);
  border-left: 1px solid #606266;
  border-right: 1px solid #606266;
  flex-direction: column;
}

.container .content-body .map-content-item .detail-area .detail-table .second-type .second-type-right {
  color: #409eff;
  text-align: center;
  width: calc(10% - 1px);
}

.container .content-body .map-content-item .detail-area .detail-switch {
  color: #409eff;
  margin-top: 20rpx;
}

.container .detail-map {
  position: fixed;
  top: 0;
  right: 0;
  left: 0;
  bottom: 0;
  background: rgba(48, 49, 51, 0.4);
  z-index: 100;
  display: flex;
  justify-content: center;
  align-items: center;
}

.container .detail-map .map-in-big-size {
  width: 100vw;
  transform: rotate(90deg) scale(1.5);
  background-color: white;
  border-radius: 10rpx;
  padding: 20rpx;
}

.drop-dialog-mask {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: rgba(48, 49, 51, 0.4);
  z-index: 100;
  display: flex;
  justify-content: center;
  align-items: center;
}

.drop-dialog-mask .dialog {
  background: #f2f6fc;
  width: 70vw;
  height: 80vh;
  border-radius: 20rpx;
  padding: 40rpx 20rpx;
  overflow: scroll;
  display: flex;
  flex-direction: column;
}

.drop-dialog-mask .dialog .description {
  padding-bottom: 30rpx;
}
