.container {
  overflow: scroll;
  background-color: #f2f6fc;
}

.container input {
  border: 1px solid #909399;
  border-radius: 10rpx;
  padding-left: 10rpx;
}

.container input {
  background-color: white;
}

.container .input-area {
  height: auto;
  width: calc(100% - 40rpx);
  margin-top: 40rpx;
  border: 1px solid #606266;
  border-radius: 20rpx;
  padding: 10rpx 20rpx 0 20rpx;
}

.container .input-area .input-panel {
  font-size: 30rpx;
  margin-bottom: 60rpx;
}

.container .input-area .input-panel .title {
  font-weight: bold;
  border-bottom: 1px solid #909399;
  line-height: 60rpx;
}

.container .input-area .input-panel .base-params-container {
  padding: 10rpx 20rpx;
}

.container .input-area .input-panel .base-params-container .base-param {
  margin: 10rpx;
  display: flex;
  align-items: center;
}

.container .input-area .input-panel .base-params-container .base-param label {
  width: 50%;
}

.container .input-area .input-panel .base-params-container input {
  width: 50%;
}

.container .input-area .input-panel .formular-item label {
  font-size: 24rpx;
}

.container .input-area .input-panel .formular-item {
  padding: 10rpx;
}

.container .input-area .coefficient-param-panel .coefficient-params-container {
  padding: 10rpx 0;
  font-size: 32rpx;
  margin-bottom: -20rpx;
}

.container .input-area .coefficient-param-panel .coefficient-params-container .coefficient-param-container {
  margin-bottom: 20rpx;
  display: flex;
  padding: 0 20rpx;
  align-items: center;
}

.container .input-area .coefficient-param-panel .coefficient-params-container .coefficient-param-container .coefficient-title {
  font-size: 26rpx;
  font-weight: bold;
  width: 40%;
}

.container .input-area .coefficient-param-panel .coefficient-params-container .coefficient-param-container .option-container {
  width: 60%;
}

.container .input-area .coefficient-param-panel .coefficient-params-container .coefficient-param-container radio-group {
  display: flex;
  flex-direction: column;
  margin-left: -12%;
}

.container .input-area .coefficient-param-panel .coefficient-params-container radio {
  transform: scale(0.6);
  height: 48rpx;
}

.container .input-area .cartridge-coefficient-container .cartridge-container {
  display: flex;
}

.container .input-area .cartridge-coefficient-container .cartridge-container .cartridge-percent {
  border: 1px solid #606266;
  margin: 0 4rpx;
  width: 30rpx;
  border-radius: 5rpx;
  background-color: white;
  height: 40rpx;
}

.container .input-area .cartridge-coefficient-container .cartridge-container .selected {
  background-color: #409eff;
}

.container .input-area .coefficient-param-panel .coefficient-params-container .coefficient-param-container .base-option label {
  font-size: 24rpx;
}

.container .input-area .coefficient-param-panel .coefficient-params-container .coefficient-param-container .base-option text {
  font-size: 24rpx;
}

.container .input-area .coefficient-param-panel .coefficient-params-container .coefficient-param-container .base-option input {
  width: 20%;
  font-size: 24rpx;
  font-weight: normal;
}

.container .input-area .float-coefficient-container .float-container .overweight-bullet-switch label {
  font-weight: bold;
}

.container .input-area .float-coefficient-container .float-container .overweight-bullet-switch switch {
  margin-left: 10rpx;
  transform: scale(0.6);
}

.container .input-area .additional-param-panel .additional-params-container {
  padding: 10rpx 20rpx;
}

.container .input-area .additional-param-panel .additional-params-container .additional-param {
  margin: 10rpx 0;
  display: flex;
  align-items: center;
}

.container .input-area .additional-param-panel .additional-params-container .additional-param .additional-param-title {
  font-size: 26rpx;
  font-weight: bold;
  width: 40%;
}

.container .input-area .additional-param-panel .additional-params-container .additional-param .additional-param-input-area {
  display: flex;
  align-items: center;
  width: 60%;
}

.container .input-area .additional-param-panel .additional-params-container .additional-param .additional-param-input-area {
  width: 60%;
}

.container .input-area .additional-param-panel .additional-params-container .additional-param .column {
  display: flex;
  flex-direction: column;
  width: 60%;
}

.container .input-area .additional-param-panel .additional-params-container .additional-param .column label {
  font-size: 24rpx;
}

.container .input-area .additional-param-panel .additional-params-container .additional-param .column input {
  width: 20%;
  font-size: 24rpx;
  font-weight: normal;
}

.container .input-area .additional-param-panel .additional-params-container .additional-param .additional-param-input-area input {
  font-size: 26rpx;
  width: 100%;
}

.container .get-damage-result-container {
  margin-top: 40rpx;
  display: flex;
  justify-content: center;
  align-items: center;
}

.container .get-damage-result-container .get-damage-result {
  color: white;
  background-color: #409eff;
  border-radius: 10rpx;
  padding: 20rpx 40rpx;
}

.container .get-damage-result-container .disabled {
  background-color: #C0C4CC;
}

.result-panel {
  position: fixed;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background-color: rgba(48, 49, 51, 0.5);
  z-index: 10;
}

.result-panel .result-table {
  position: absolute;
  top: 10vh;
  left: 10vw;
  right: 10vw;
  height: 80vh;
  background-color: white;
  border-radius: 20rpx;
  padding: 20rpx;
  display: flex;
  flex-direction: column;
}

.result-panel .result-table .result-item {
  flex-grow: 1;
  display: flex
}

.result-panel .result-table .result-item .result-title {
  font-size: 32rpx;
  width: 50%;
}

.result-panel .result-table .result-item .result-value {
  font-size: 30rpx;
  width: 50%;
}

.result-panel .result-table .result-item .important {
  color: #409eff;
}